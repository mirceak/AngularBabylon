{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@kernel/*": [
        "./src/kernel/*"
      ],
      "@custom/*": [
        "./src/custom/*"
      ],
      "@final/*": [
        "./src/final/*"
      ]
    }
  },
  "files": [
    "src/main.single-spa.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}